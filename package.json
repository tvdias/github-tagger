{
  "name": "github-tagger",
  "version": "0.0.2",
  "private": true,
  "description": "Tags the commit with a given text",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "test": "jest",
    "package": "ncc build",
    "all": "npm run format && npm run build && npm run package && npm run test && npm run format-check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tvdias/github-tagger.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Thiago Vaz Dias",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.0.0",
    "@actions/github": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.4",
    "husky": "^2.7.0",
    "jest": "^24.8.0",
    "jest-circus": "^24.7.1",
    "prettier": "^1.17.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.1"
  }
}
